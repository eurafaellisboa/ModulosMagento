<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin"> 
                <resource id="FME_Extensions::fmeextensions" title="FME Extensions">
                    <resource id="FME_ShareCart::main_menu" title="Share cart" 
                                          sortOrder="100">
                        <resource id="FME_ShareCart::savedcart" title="Manage Sharecart" 
                                  sortOrder="22" />             
                    </resource>
                </resource>                 
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">                             
                                <resource id="FME_ShareCart::sharecart_config" title="Share Cart Configuration" />                            
                        </resource>
                    </resource>
                </resource>
            </resource> 
        </resources>
    </acl>
</config>
 